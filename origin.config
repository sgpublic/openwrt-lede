#              斐讯 k2p            #
CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7621=y
CONFIG_TARGET_ramips_mt7621_DEVICE_phicomm_k2p=y

#               基础               #
CONFIG_LUCI_LANG_zh_Hans=y
CONFIG_PACKAGE_default-settings=y
# 切换 WiFi 闭源驱动
CONFIG_PACKAGE_kmod-mt7615d=y
CONFIG_PACKAGE_kmod-mt7615d_dbdc=y
# CONFIG_PACKAGE_kmod-mt7615e is not set
# CONFIG_PACKAGE_kmod-mt7615-firmware is not set
# 缓存工具链
CONFIG_DEVEL=y
CONFIG_CCACHE=y

#               主题               #
CONFIG_PACKAGE_luci-app-argon=y
CONFIG_PACKAGE_luci-app-argon-config=y

#               插件               #
CONFIG_PACKAGE_luci-app-ssr-plus=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_ShadowsocksR_Libev_Client=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_V2ray_Plugin=y

#               其他               #
# 解决 luci-app-argon-config 依赖缺失
CONFIG_PACKAGE_luci-lib-ipkg=y
# 解决 luci-app-ssr-plus 依赖缺失
CONFIG_PACKAGE_iptables=y
CONFIG_PACKAGE_iptables-mod-tproxy=y
# 解决 luci-app-ssr-plus 代理失效
CONFIG_PACKAGE_kmod-ipt-nat=y
# 解决 luci-app-ssr-plus 找不到 v2ray
CONFIG_PACKAGE_v2ray-core=y
# 解决 dnsmasq 和 dnsmasq-full 冲突
# CONFIG_PACKAGE_dnsmasq is not set

